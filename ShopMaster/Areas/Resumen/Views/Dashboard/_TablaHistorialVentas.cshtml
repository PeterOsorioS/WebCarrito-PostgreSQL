@model IEnumerable<Venta>

@foreach (var item in Model)
{
    foreach (var detalle in item.DetalleVenta)
    {
        <tr>
            <th class="font-weight-normal text-left">@item.FechaVenta</th>
            <th class="font-weight-normal text-left">@item.Contacto</th>
            <th class="font-weight-normal text-left">@detalle.Producto.Nombre</th>
            <th class="font-weight-normal text-left">@detalle.Producto.Precio.ToString("$#,0")</th>
            <th class="font-weight-normal text-left">@detalle.Cantidad</th>
            <th class="font-weight-normal text-left">@item.MontoTotal.ToString("$#,0")</th>
        </tr>
    }
}
